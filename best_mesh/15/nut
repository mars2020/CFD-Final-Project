FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "15";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    airfoil
    {
        type            calculated;
        value           uniform 0;
    }
    farBoundaries
    {
        type            freestream;
        freestreamValue uniform 0;
        value           uniform 0;
    }
    inlet
    {
        type            freestream;
        freestreamValue uniform 0;
        value           uniform 0;
    }
    outlet
    {
        type            freestream;
        freestreamValue uniform 0;
        value           uniform 0;
    }
    defaultFaces
    {
        type            empty;
    }
}


